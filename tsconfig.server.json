{
    "extends": "./tsconfig",  
    "include": ["server"],
    "compilerOptions": {
        "module":"commonjs",
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "lib": ["es6", "dom"],
        "target": "es6",
        "jsx": "react",
        "allowJs": true,
        "strict": true,
        "strictNullChecks": true,
        "noImplicitAny": true,
        "experimentalDecorators": true,
    },
    "exclude": ["node_modules"]
}